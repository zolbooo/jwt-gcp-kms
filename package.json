{
  "name": "jwt-gcp-kms",
  "version": "1.2.8",
  "description": "JWT library with GCP KMS support",
  "type": "module",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:cjs && npm run build:esm && ./fixup.sh",
    "test": "jest",
    "test-e2e": "jest --config ./e2e/jest.config.js",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zolbooo/jwt-gcp-kms.git"
  },
  "keywords": [
    "gcp",
    "google-cloud",
    "kms",
    "security",
    "crypto",
    "jwt",
    "jsonwebtoken"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "author": "Zolboobayar Gantumur <zolboobayar@metainc.mn>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zolbooo/jwt-gcp-kms/issues"
  },
  "homepage": "https://github.com/zolbooo/jwt-gcp-kms#readme",
  "engines": {
    "node": ">=18.0.0 <19.0.0 || >=20.0.0 <21.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/ms": "^0.7.32",
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-crypto/crc32c": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3"
  },
  "peerDependencies": {
    "@google-cloud/kms": "^4.0.1"
  }
}
